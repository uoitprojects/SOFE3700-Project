--STORES BOOK DATA ... REFERENCES AUTHOR




CREATE TABLE Book
(
isbn int NOT NULL,
title string NOT NULL,
author_first_name string NOT NULL,
author_last_name string NOT NULL,
author_other_name string,
publication_date string,
page_length int NOT NULL,
category string, 
publisher string NOT NULL,
type string,
series string, 
PRIMARY KEY (isbn),
FOREIGN KEY ("author_first_name") references Author(first_name),
FOREIGN KEY ("author_last_name") references Author(last_name),
FOREIGN KEY ("author_other_name") references Author(other_name)
);




