#STORES BOOK DATA ... REFERENCES AUTHOR




CREATE TABLE Book
(
isbn INT NOT NULL,
title VARCHAR(50) NOT NULL,
author_id INT NOT NULL,
publication_date VARCHAR(30),
page_length INT NOT NULL,
category VARCHAR(30),
publisher VARCHAR(30),
book_type VARCHAR(30),
series VARCHAR(30),
PRIMARY KEY (isbn),
FOREIGN KEY (author_id) references Author(id)
);




